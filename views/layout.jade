!!!
html
  include inc/header
  body
    div.navbar.navbar-fixed-top
      div.navbar-inner
        div.container
          a(data-toggle='collapse', data-target='.nav-collapse').btn.btn-navbar
            span.icon-bar
            span.icon-bar
            span.icon-bar
          a(href='#').brand #{brand}
          ul.nav
            - var obj = { '/':'Home', '/logout':'Logout' }
              - each val, key in obj
                - if (id == key)
                  li.active
                    a(href='#{key}') #{val}
                - else            
                  li
                    a(href='#{key}') #{val}
    div.container
      block content
      include inc/footer

    include inc/scripts
